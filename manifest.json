{
  "manifest_version": 2,
  "name": "Zoom Zapper",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "version": "1.0",
  "permissions": [
    "*://*.zoom.us/",
    "background",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
